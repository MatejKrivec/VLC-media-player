<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="Image" x:Key="Image_UserControll_previous">
        <Setter Property="Height" Value="45"></Setter>
        <Setter Property="Width" Value="43"></Setter>
    </Style>
    <Style TargetType="Image" x:Key="Image_UserControll_next">
        <Setter Property="Height" Value="45"></Setter>
        <Setter Property="Width" Value="43"></Setter>
    </Style>
    <Style TargetType="Image" x:Key="Image_UserControll_play">
        <Setter Property="Height" Value="32"></Setter>
        <Setter Property="Width" Value="38"></Setter>
    </Style>
    <Style TargetType="Image" x:Key="Image_UserControll_pause">
        <Setter Property="Height" Value="48"></Setter>
        <Setter Property="Width" Value="55"></Setter>
    </Style>
    <Style TargetType="Image" x:Key="Image_UserControll_stop">
        <Setter Property="Height" Value="30"></Setter>
        <Setter Property="Width" Value="38"></Setter>
    </Style>

    <Style TargetType="Image" x:Key="ListView_Image">
        <Setter Property="Width" Value="50"></Setter>
        <Setter Property="Height" Value="20"/>
    </Style>


    <Style TargetType="Image" x:Key="Image_uredi">
        <Setter Property="Height" Value="280"></Setter>
        <Setter Property="Width" Value="524"></Setter>
    </Style>

</ResourceDictionary>