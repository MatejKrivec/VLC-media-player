<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="TextBox" x:Key="Nastavitve_textbox">
        <Setter Property="Width" Value="105"></Setter>
        <Setter Property="Margin" Value="0,0,40,0"></Setter>
        <Setter Property="Height" Value="30"></Setter>
        <Setter Property="HorizontalAlignment" Value="Right"></Setter>
    </Style>


    <Style TargetType="TextBox" x:Key="Uredi_textbox1">
        <Setter Property="MinWidth" Value="90"></Setter>
        <Setter Property="Margin" Value="5"></Setter>

    </Style>
    <Style TargetType="TextBox" x:Key="Uredi_textbox2">
        <Setter Property="MinWidth" Value="90"></Setter>
        <Setter Property="Margin" Value="5"></Setter>
        <Setter Property="Width" Value="150"></Setter>

    </Style>
</ResourceDictionary>