<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="DockPanel" x:Key="DockPanel_casovnik">
        <Setter Property="HorizontalAlignment" Value="Center"></Setter>
        <Setter Property="Width" Value="auto"></Setter>
    </Style>


    <Style TargetType="DockPanel" x:Key="DockPanel_Slider">
        <Setter Property="HorizontalAlignment" Value="Right"></Setter>
        <Setter Property="Width" Value="120"></Setter>

    </Style>





    <Style TargetType="DockPanel" x:Key="DockPanel_Nastavitve_gumbi">
        <Setter Property="Margin" Value="0,38,0,0"></Setter>
        <Setter Property="Width" Value="355"></Setter>
        <Setter Property="Background" Value="Gray"></Setter>
    </Style>



    <Style TargetType="DockPanel" x:Key="DockPanel_UserControll">
        <Setter Property="Margin" Value="2"></Setter>
        <Setter Property="Width" Value="275"></Setter>
        <Setter Property="HorizontalAlignment" Value="Center"></Setter>
    </Style>
</ResourceDictionary>