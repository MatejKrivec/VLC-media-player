<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="Label" x:Key="Label_Properties1">
        <Setter Property="Width" Value="55"></Setter>
        <Setter Property="Margin" Value="55,10,0,0"> </Setter>
        <Setter Property="HorizontalAlignment" Value="Left"></Setter> 
    </Style>
    <Style TargetType="Label" x:Key="Label_Properties2">
        <Setter Property="Width" Value="auto"></Setter>
        <Setter Property="Margin" Value="0,10,20,0"></Setter>
        <Setter Property="HorizontalAlignment" Value="Right"></Setter>
    </Style>


    <Style TargetType="Label" x:Key="Label_Uredi">
        <Setter Property="Width" Value="55"></Setter>
    </Style>
    <Style TargetType="Label" x:Key="Label_Uredi_combobox">
        <Setter Property="Width" Value="55"></Setter>
        <Setter Property="Margin" Value="0,20,0,0"></Setter>
    </Style>
    
</ResourceDictionary>